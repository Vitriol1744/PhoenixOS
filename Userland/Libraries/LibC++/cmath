#pragma once

#undef ceil
#undef isnan
#undef isfinite
#undef signbit

namespace std
{
    inline constexpr auto ceil(auto val) { return __builtin_ceil(val); }

    inline constexpr auto isnan(auto val) { return __builtin_isnan(val); }

    inline constexpr auto isfinite(auto val) { return __builtin_isfinite(val); }

    inline constexpr auto signbit(auto val) { return __builtin_signbit(val); }
} // namespace std